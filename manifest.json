{
  "manifest_version": 2,
  "name": "Amazon Tools",
  "version": "1.0",
  "description": "tool to help search data",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png"
        },
  "content_scripts": [
    {
      "matches": [
        "*://www.amazonlogistics.com/comp/packageScan?eventId=ReceiveScan&subMenu=ReceiveScan*"
      ],
      "js": ["jquery-3.2.1.min.js", "backtostation.js" ]
    }
  ],
  "background":
  {
    "scripts": ["jquery-3.2.1.min.js"]
  }
}
