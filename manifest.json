{
  "manifest_version": 2,
  "name": "Virtual Scan",
  "version": "2.0",
  "description": "Virtual Scan for return to station",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png"
        },
  "content_scripts": [
   {
      "matches": [
        "*://logistics.amazon.com/internal/network/returntostation/*"
      ],
      "js": ["jquery-3.2.1.min.js", "backtostation.js" ]
    }
  ],
  "background":
  {
    "scripts": ["jquery-3.2.1.min.js"]
  }
}
